<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.margo.saveactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityTwo"
            android:label="ActivityTwo" />
        <activity
            android:name=".ActivityThree"
            android:label="ActivityThree" />
        <activity
            android:name=".ActivitySeven"
            android:label="ActivitySeven" />
        <activity android:name=".ActivityFour" />
        <activity android:name=".ActivityFive" />
        <activity android:name=".ActivitySix"></activity>
    </application>

</manifest>